#lang racket
(define calls 0)
(define (force f)
  (f))
(define s (cons 1 (lambda () (add s s))))
(define (add s1 s2)
  (set! calls (+ calls 1))
  (display "calling scale ")
  (display calls)
  (display "\n")
  (cons (+ (car s1) (car s2)) (lambda () (add (cdrs s1) (cdrs s2)))))
(define (cdrs s)
  (force (cdr s)))


